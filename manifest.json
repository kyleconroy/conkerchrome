{
  "name": "Conker Chrome",
  "version": "1.0",
  "description": "Conkeror Emulation in Chrome",
  "browser_action": {
    "default_icon": "icon.png"
  },
  "content_scripts": [
      {
	  "matches": ["<all_urls>"],
	  "css": ["style.css"],
	  "js": ["jquery.js","jquery.hotkeys.js","conkeror.js"]
      }
  ],
  "permissions": [
      "*://*/*",
      "tabs",
      "notifications"
  ]
}